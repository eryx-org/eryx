{
  "name": "@bsull/eryx",
  "type": "module",
  "description": "A Python sandbox powered by WebAssembly, for browser and Node.js",
  "version": "0.4.0",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eryx-org/eryx.git",
    "directory": "js/eryx"
  },
  "files": [
    "interfaces/*",
    "shims/*",
    "*.wasm",
    "*.js",
    "*.d.ts",
    "python-stdlib.tar.gz",
    "README.md"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./callbacks": {
      "types": "./shims/callbacks.d.ts",
      "default": "./shims/callbacks.js"
    },
    "./low-level": {
      "types": "./eryx-sandbox.d.ts",
      "default": "./eryx-sandbox.js"
    }
  },
  "keywords": [
    "python",
    "sandbox",
    "wasm",
    "webassembly",
    "component-model",
    "interpreter"
  ],
  "dependencies": {
    "@bytecodealliance/preview2-shim": "^0.17.0"
  }
}
